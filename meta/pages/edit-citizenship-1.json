        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "name": "b1.getCitizenshipByID",
        "parameters": [
          {
            "name": "ID",
            "value": "${parameters.ID}"
          }
        ],
        "then": [
          {
            "name": "citizenship",
            "type": "set",
            "value": "${result}",
            "valueType": "B.Models.B1.Citizenship"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
